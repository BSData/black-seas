    <selectionEntry id="2057-2e79-404a-fbca" name="National Special Rules" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="42f1-6841-0017-3368" type="max"/>
